		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment",
				"string.comment"
			],
			"settings": {
				"foreground": "#6a737d"
			}
		},
		{
			"name": "Constant",
			"scope": [
				"constant",
				"entity.name.constant",
				"variable.other.constant",
				"variable.language"
			],
			"settings": {
				"foreground": "#005cc5"
			}			
		},
		{
			"name": "Clojure workaround; don't highlight these separately from their enclosing scope",
			"scope": [
				"keyword.operator.symbole",
				"keyword.other.mark"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Entity",
			"scope": [
				"entity",
				"entity.name"
			],
			"settings": {
				"foreground": "#6f42c1"
			},			
		},
		{
			"scope": [
				"variable.parameter.function"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"scope": [
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#22863a"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword"
			],
			"settings": {
				"foreground": "#d73a49"
			},			
		},
		{
			"name": "Storage",
			"scope": [
				"storage",
				"storage.type"
			],
			"settings": {
				"foreground": "#d73a49"
			},			
		},
		{
			"scope": [
				"storage.modifier.package",
				"storage.modifier.import",
				"storage.type.java"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"punctuation.definition.string",
				"string punctuation.section.embedded source"
			],
			"settings": {
				"foreground": "#00733B"
				// "foreground": "#032f62"
			},			
		},
		{
			"name": "Ada workaround; don't highlight imports as strings",
			"scope": [
				"string.unquoted.import.ada"
			],
			"settings": {}
		},
		{
			"name": "Support",
			"scope": [
				"support"
			],
			"settings": {
				"foreground": "#005cc5"
			},			
		},
		{
			"scope": [
				"meta.property-name"
			],
			"settings": {
				"foreground": "#005cc5"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#e36209"
			},			
		},
		{
			"scope": [
				"variable.other"
			],
			"settings": {
				"foreground": "#000000"
			}
		},
		{
			"name": "Invalid - Broken",
			"scope": [
				"invalid.broken"
			],
			"settings": {
				"fontStyle": "bold italic underline",
				"foreground": "#b31d28"
			},			
		},
		{
			"name": "Invalid – Deprecated",
			"scope": [
				"invalid.deprecated"
			],
			"settings": {
				"fontStyle": "bold italic underline",
				"foreground": "#b31d28"
			},			
		},
		{
			"name": "Invalid – Illegal",
			"scope": [
				"invalid.illegal"
			],
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#b31d28"
			},			
		},
		{
			"name": "Carriage Return",
			"scope": [
				"carriage-return"
			],
			"settings": {
				"fontStyle": "italic underline",
				"foreground": "#d73a49"
			},			
		},
		{
			"name": "Invalid - Unimplemented",
			"scope": [
				"invalid.unimplemented"
			],
			"settings": {
				"fontStyle": "bold italic underline",
				"foreground": "#b31d28"
			},			
		},
		{
			"scope": [
				"message.error"
			],
			"settings": {
				"foreground": "#b31d28"
			}
		},
		{
			"name": "String embedded-source",
			"scope": [
				"string source"
			],
			"settings": {
				"foreground": "#000000"
			},			
		},
		{
			"name": "String variable",
			"scope": [
				"string variable"
			],
			"settings": {
				"foreground": "#005cc5"
			},			
		},
		{
			"name": "String.regexp",
			"scope": [
				"source.regexp",
				"string.regexp"
			],
			"settings": {
				"foreground": "#032f62"
			},			
		},
		{
			"name": "String.regexp.special",
			"scope": [
				"string.regexp.character-class",
				"string.regexp constant.character.escape",
				"string.regexp source.ruby.embedded",
				"string.regexp string.regexp.arbitrary-repitition"
			],
			"settings": {
				"foreground": "#032f62"
			},			
		},
		{
			"name": "String.regexp constant.character.escape",
			"scope": [
				"string.regexp constant.character.escape"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#22863a"
			},			
		},
		{
			"name": "Support.constant",
			"scope": [
				"support.constant"
			],
			"settings": {
				"foreground": "#005cc5"
			},			
		},
		{
			"name": "Support.variable",
			"scope": [
				"support.variable"
			],
			"settings": {
				"foreground": "#005cc5"
			},			
		},
		{
			"name": "meta module-reference",
			"scope": [
				"meta.module-reference"
			],
			"settings": {
				"foreground": "#005cc5"
			},			
		},
		{
			"name": "Markup.list",
			"scope": [
				"markup.list"
			],
			"settings": {
				"foreground": "#735c0f"
			},			
		},
		{
			"name": "Markup.heading",
			"scope": [
				"markup.heading",
				"markup.heading entity.name"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#005cc5"
			},			
		},
		{
			"name": "Markup.quote",
			"scope": [
				"markup.quote"
			],
			"settings": {
				"foreground": "#22863a"
			},
		},
		{
			"name": "Markup.italic",			
			"scope": [
				"markup.italic"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#000000"
			},
		},
		{
			"name": "Markup.bold",
			"scope": [
				"markup.bold"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#000000"
			},			
		},
		{
			"name": "Markup.raw",
			"scope": [
				"markup.raw"
			],
			"settings": {
				"foreground": "#005cc5"
			},			
		},
		{
			"name": "Markup.deleted",
			"scope": [
				"markup.deleted",
				"meta.diff.header.from-file",
				"punctuation.definition.deleted"
			],
			"settings": {
				"foreground": "#b31d28"
			},			
		},
		{
			"name": "Markup.inserted",
			"scope": [
				"markup.inserted",
				"meta.diff.header.to-file",
				"punctuation.definition.inserted"
			],
			"settings": {
				"foreground": "#22863a"
			},			
		},
		{
			"scope": [
				"markup.changed",
				"punctuation.definition.changed"
			],
			"settings": {
				"foreground": "#e36209"
			}
		},
		{
			"scope": [
				"markup.ignored",
				"markup.untracked"
			],
			"settings": {
				"foreground": "#005cc5"
			}
		},
		{
			"scope": [
				"meta.diff.range"
			],
			"settings": {
				"foreground": "#6f42c1",
				"fontStyle": "bold"
			}
		},
		{
			"scope": [
				"meta.diff.header"
			],
			"settings": {
				"foreground": "#005cc5"
			}
		},
		{			
			"name": "Meta.separator",
			"scope": [
				"meta.separator"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#005cc5"
			},			
		},
		{
			"name": "Output",
			"scope": [
				"meta.output"
			],
			"settings": {
				"foreground": "#005cc5"
			}
		},
		{
			"scope": [
				"brackethighlighter.tag",
				"brackethighlighter.curly",
				"brackethighlighter.round",
				"brackethighlighter.square",
				"brackethighlighter.angle",
				"brackethighlighter.quote"
			],
			"settings": {
				"foreground": "#586069"
			}
		},
		{
			"scope": [
				"brackethighlighter.unmatched"
			],
			"settings": {
				"foreground": "#b31d28"
			}
		},
		{
			"scope": [
				"sublimelinter.mark.error"
			],
			"settings": {
				"foreground": "#b31d28"
			}
		},
		{
			"scope": [
				"sublimelinter.mark.warning"
			],
			"settings": {
				"foreground": "#e36209"
			}
		},
		{
			"scope": [
				"sublimelinter.gutter-mark"
			],
			"settings": {
				"foreground": "#959da5"
			}
		},
		{
			"scope": [
				"constant.other.reference.link",
				"string.other.link"
			],
			"settings": {
				"foreground": "#032f62",
				"fontStyle": "underline"
			}
		},
		{
			"scope": [
				"meta.function-call support.function",
				"meta.function-call entity.name.function"
			],
			"settings": {
				"foreground": "#005cc5"
			}
		},
		{
			"scope": [
				"keyword.operator"
			],
			"settings": {
